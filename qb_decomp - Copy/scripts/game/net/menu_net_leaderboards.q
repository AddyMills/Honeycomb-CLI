script leaderboard_loading_bar_animate 
	Obj_Getid
	Wait \{0.5 Seconds}
	<objId> :SetProps Alpha = 1
	I = 0
	begin
	Text = "Loading"
	switch (<I>)
		case 0
		Text = "Loading"
		case 1
		Text = "Loading."
		case 2
		Text = "Loading.."
		case 3
		Text = "Loading..."
	endswitch
	<objId> :SetProps Text = <Text>
	Wait \{0.5 Seconds}
	I = (<I> + 1)
	if (<I> > 3)
		I = 0
	endif
	repeat
endscript

