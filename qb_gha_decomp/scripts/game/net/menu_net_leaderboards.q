script leaderboard_loading_bar_animate 
	obj_getid
	wait \{0.5 seconds}
	<ObjID> :setprops alpha = 1
	i = 0
	begin
	text = "Loading"
	switch (<i>)
		case 0
		text = "Loading"
		case 1
		text = "Loading."
		case 2
		text = "Loading.."
		case 3
		text = "Loading..."
	endswitch
	<ObjID> :setprops text = <text>
	wait \{0.5 seconds}
	i = (<i> + 1)
	if (<i> > 3)
		i = 0
	endif
	repeat
